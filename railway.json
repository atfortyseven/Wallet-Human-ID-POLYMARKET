{
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "npm run build",
        "nixpacksPlan": {
            "providers": [
                "node"
            ],
            "variables": {
                "NIXPACKS_NODE_VERSION": "22"
            }
        }
    },
    "deploy": {
        "startCommand": "npm start"
    }
}